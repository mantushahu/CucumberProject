pipeline{
agent any
stages{
stage('Build'){
steps {withMaven(maven : 'maven_3_6_3'){echo "Building the code......"
sh "mvn clean"}
}}
stage('Test'){
steps {withMaven(maven : 'maven_3_6_3'){echo "Testing the code......"
sh "mvn test"}}
}
stage('Compile'){
steps {withMaven(maven : 'maven_3_6_3'){echo "Compiling the code......"
sh "mvn clean compile"}
}
}
stage('Deploy'){
steps {withMaven(maven : 'maven_3_6_3'){echo "Building the code......"
sh "mvn deploy"}
}}
}}