pipeline{
agent any
tools {
        maven '"maven_3_6_3"'
    }
stages{
stage('Compile'){
steps {echo "Compiling the code......"
bat "mvn clean compile"}
}

stage('Test'){
steps {echo "Testing the code......"
bat "mvn test"}
}
stage('Deploy'){
steps {echo "Building the code......"
bat "mvn deploy"}
}
}}